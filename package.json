   "dependencies": {
      "bcryptjs": "^2.4.3",
      "colors": "^1.4.0",
      "cors": "^2.8.5",
      "crypto": "^1.0.1",
      "dotenv": "^10.0.0",
      "express": "^4.17.1",
      "git": "^0.1.5",
      "jsonwebtoken": "^8.5.1",
      "mongoose": "^6.0.12",
      "randomatic": "^3.1.1",
      "react-hierarchy-tree-graph": "^1.0.3"
   },
   "devDependencies": {
      "nodemon": "^2.0.14"
   }
}
